{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/contact/route.ts": {
      "maxDuration": 10
    },
    "app/api/resume/route.ts": {
      "maxDuration": 5
    }
  },
  "headers": [
    {
      "source": "/kamalsaitillarigenai10.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "inline; filename=\"Kamal_Sai_Tillari_Resume.pdf\""
        }
      ]
    }
  ]
}
